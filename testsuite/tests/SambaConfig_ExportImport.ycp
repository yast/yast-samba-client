/**
 * test SambaConfig::Export:
 * should remove modified flags and internal shares from config
 */
{
  import "Testsuite";
  import "SambaConfig";

  map i = $[
    "a" : $[
      "Bee Bee"         : "x",
      "_modified"       : true,
      "_disabled"       : true,
      "_xxx"            : 8,
      "_comment"        : "A"
    ],
    "_internal"         : $[
      "abc"             : "ABC"
    ],
    "removed"           : nil,
    "b"                 : $[
      "no"              : nil,
      "Two Two"         : 22
    ]
  ];

  Testsuite::Dump (i);

  Testsuite::Test(``(SambaConfig::Import (i)), [ $[], $[], $[] ], 0);

  Testsuite::Test (``(SambaConfig::Export ()), [], 0);
}
